{
  "name": "counseling-training-chatbot",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "package": "vite package",
    "preview": "vite preview",
    "postinstall": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "update-types": "dotenv -- cross-var npx openapi-typescript %VITE_SUPABASE_URL%/rest/v1/ --header apiKey:%VITE_SUPABASE_ANON_KEY% --output types/database/index.ts && prettier --write --plugin-search-dir=. types/database/index.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "1.0.0-next.403",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/svelte": "^3.1.0",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.7",
    "cross-var": "^1.1.0",
    "daisyui": "^2.14.3",
    "dotenv-cli": "^5.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "jest": "^27.5.1",
    "postcss": "^8.4.13",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "prisma": "^4.3.1",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-jester": "^2.3.2",
    "svelte-multiselect": "^6.0.3",
    "svelte-preprocess": "^4.10.1",
    "tailwindcss": "^3.1.7",
    "ts-jest": "^27.1.4",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "@supabase/auth-helpers-svelte": "^0.4.5",
    "@supabase/auth-helpers-sveltekit": "^0.6.8",
    "@supabase/supabase-js": "^1.35.2",
    "openai": "^3.0.0",
    "pdfkit": "^0.13.0",
    "blob-stream": "^0.1.3",
    "browserify": "^17.0.0",
    "jspdf": "^2.5.1",
    "svelte-simple-modal": "^1.4.1",
    "vite": "^3.0.4"
  }
}
